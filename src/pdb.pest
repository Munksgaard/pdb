WHITESPACE = _{ " " | "\n" }

identifier = { ('a'..'z')+ }


tyident = { 'A'..'Z' ~ ASCII_ALPHANUMERIC* }

ty = { tyident }

table = { "table" ~ ty }

int = @{
    "-"?
    ~ (ASCII_NONZERO_DIGIT ~ ASCII_DIGIT*)
}

bool = { "true" | "false" }

expr = { int | bool }

insert = { "insert" ~ expr }

select = { "select" }

statement = _{ SOI ~ (insert | select) ~ EOI }